body
  font: 12px Helvetica, Arial, sans-serif

border-radius()
  -webkit-border-radius: arguments
  -moz-border-radius: arguments
  border-radius: arguments

selected-highlight-color = #9fdf9f

button
  border-radius: 5px

.featureList div.selected


.featureList div.selected:before
  content: "\2713"
  color: darkgreen

.featureList button.selected
  background-color: selected-highlight-color

.featureList ul
  list-style-type: none;
